- var tarea = assign
- var aula = classroom
doctype html

html
  include ../layouts/headParams2.pug
  body
    include ../layouts/nav.pug       
    div(class="container")
      div(class="row")
        div(class="col s12", id="space")  
        div(align="center" class="col s12")  
          p(class="ftxt") Haga click en el siguiente botón para aceptar la tarea 
            span(id="tarea") #{tarea}
            span  del aula 
            span(id="aula") #{aula}
        div(class="col s12", id="ispace")  
        div(align="center" class="col s12")  
          a(id="click" class="waves-effect waves-light btn-large light-blue darken-4") Aceptar invitación
                   
    include ../layouts/footerParams2.pug
    script(type="text/javascript", src="../../js/getAssign.js")
